{
    "manifest_version": 3,
    "name": "Disable Blur Event",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "main.js"
            ],
            "run_at": "document_start",
            "world": "MAIN"
        }
    ]
}